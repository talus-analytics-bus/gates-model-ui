{
  "name": "flood-model-ui",
  "version": "0.1.0",
  "description": "User interface for a prototype flood model",
  "private": true,
  "main": "gulpfile.js",
  "scripts": {
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talus-analytics-gov/flood-model-ui.git"
  },
  "bugs": {
    "url": "https://github.com/talus-analytics-gov/flood-model-ui/issues"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "eslint": "~2.5.3",
    "eslint-config-airbnb": "~6.2.0",
    "gulp": "~3.9.0",
    "gulp-babel": "~6.1.2",
    "gulp-concat": "~2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-rename": "~1.2.2",
    "gulp-uglify": "~1.5.1"
  },
  "dependencies": {
    "express": "~4.13.1",
    "python-shell": "~0.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "env": {
      "jquery": true
    },
    "globals": {
      "App": true,
      "Routing": true,
      "Util": true,
      "Maps": true,
      "hasher": true,
      "crossroads": true,
      "Handlebars": true,
      "d3": true,
      "NProgress": true,
      "L": true,
      "Odometer": true
    },
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "no-use-before-define": [
        "error",
        "nofunc"
      ],
      "no-console": "off"
    }
  }
}
